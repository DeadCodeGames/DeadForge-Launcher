{
  "name": "deadforge",
  "version": "v1.4.0",
  "description": "An Electron Launcher for DeadCode Projects.",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "keywords": [],
  "author": "DeadCodeGames <404@mail.deadcode.is-a.dev> (https://deadcode.is-a.dev)",
  "contributors": [
    "Richard Kanshen <kanshen@mail.deadcode.is-a.dev> (https://kanshen.click)",
    "QlexcTheFox <80660200+musiconstereo@users.noreply.github.com> (https://github.com/MusicOnStereo)"
  ],
  "license": "BSD-3-Clause",
  "repository": "github:DeadCodeGames/DeadForge",
  "devDependencies": {
    "electron": "^29.0.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "dev.deadcode.forge",
    "productName": "DeadForge",
    "win": {
      "target": "nsis",
      "artifactName": "dev.deadcode.forge-${version}.${ext}"
    },
    "mac": {
      "target": "dmg",
      "artifactName": "dev.deadcode.forge-${version}.${ext}"
    },
    "linux": {
      "target": "deb",
      "artifactName": "dev.deadcode.forge-${version}.${ext}"
    }
  },
  "dependencies": {
    "@dead404code/lightbulbcss": "^1.0.0-Stable",
    "@electron/remote": "^2.1.2",
    "axios": "^1.6.7",
    "child_process": "^1.0.2",
    "discord-rpc": "^4.0.1",
    "downloads-folder": "^3.0.3",
    "electron-window-state": "^5.0.3",
    "follow-redirects": "^1.15.6",
    "node-fetch": "^3.3.2",
    "on-line": "^0.1.1"
  }
}
